{
  "name": "<%= projectName %>",
  "version": "1.0.0",
  "dependencies": {
    "express": "^4.18.2",
    "bcryptjs": "^2.4.3",
    "cors"  : "^2.8.5", 
    "dotenv": "^16.4.5" , 
    <% if (authMethod === "JWT") { %>
    "jsonwebtoken": "^9.0.0",
    <% } else { %>
    "express-session": "^1.17.3",
    <% } %>
    "sequelize": "^6.31.0"
    <% if (database === "sqlite") { %>
    "sqlite3": "^5.1.6"
    <% } else if (database === "mysql") { %>
    "mysql2": "^3.5.2"
    <% } else if (database === "postgres") { %>
    "pg": "^8.11.0"
    <% } %>
  },
  "scripts": {
    "start": "ts-node src/app.ts",
    "dev": "npx nodemon ./src/app.ts"
  },
  "devDependencies": {
    "typescript": "^4.4.3",
    "ts-node": "^10.9.1",
    "@types/express": "^4.17.13",
    "@types/node": "^16.10.1",
    "nodemon":"^3.1.4"
}
}
